meta {
  name: TestCreateLog
  type: http
  seq: 1
}

post {
  url: {{local_server}}/log/
  body: json
  auth: none
}

body:json {
  {
    "date": 12,
    "duration": 727,
    "name": "Test",
    "category": "POST",
    "userId": -1
  }
}

assert {
  res.status: eq 200
}

script:pre-request {
  let data = req.getBody();
  data.date = Date.now();
  
  req.setBody(data);
}

script:post-response {
  bru.setEnvVar("log_number", res.getBody().id);
}
